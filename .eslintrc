{
  "root": true,
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "plugins": [],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "semi": "error",
    "no-multiple-empty-lines": "error",
    "no-trailing-spaces": "error",
    "padded-blocks": [
      "error",
      "never"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "object-curly-newline": [
      "error",
      {
        "consistent": true
      }
    ],
    "comma-spacing": "error",
    "indent": [
      "error",
      2
    ],
    "eol-last": [
      "error",
      "always"
    ]
  }
}